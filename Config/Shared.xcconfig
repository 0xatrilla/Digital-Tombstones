// ==========================================
// Project Identity
// ==========================================
PRODUCT_NAME = KilledByGoogle
PRODUCT_DISPLAY_NAME = KilledByGoogle
PRODUCT_BUNDLE_IDENTIFIER = com.example.KilledByGoogle
MARKETING_VERSION = 1.0
CURRENT_PROJECT_VERSION = 1

// ==========================================
// Platform Configuration
// ==========================================
IPHONEOS_DEPLOYMENT_TARGET = 26.0

